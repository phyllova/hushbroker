            let iframeWebUrl = "https://m0-t.rt.prorealtime.com/ProRealTime_Lite_Auth/launch.phtml?locale=en&amp;type=WEB_PUBLIC&amp;beta=0&amp;exp=1699941403&amp;key=bdbe94d734d88ed71415d7b7664eb2e243c1689d36027f81cf7a7f15221544cc17c6f80ac28ac66094526d99d1e933655cbc1503882ac10c9794f36d1ce800e8&amp;desktop=1&amp;broker=DEMO&amp;virtual=1&amp;na=1";
            iframeWebUrl = iframeWebUrl.replace(/&amp;/g, '&');
            let iframe = document.createElement('iframe');
            iframe.setAttribute('allow', 'fullscreen *');
            iframe.setAttribute('allowFullScreen', '');
            iframe.setAttribute('src', iframeWebUrl);
            iframe.dataset.webFrameTarget = "iframe";
            iframe.classList.add('w-100', 'm-0');
            iframe.style.minHeight = '530px';
            iframe.style.minWidth = '375px';
            let currentDiv = document.getElementById('webFrame');
            currentDiv.firstElementChild.replaceWith(iframe);
        </script>
